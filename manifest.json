{
    "manifest_version": 3,
    "name": "Menzy",
    "version": "1.0.0",
    "description": "Swing Up Your SelfCare",
    "action": {
        "default_popup": "home.html",
        "default_icon": "logo.png"
    },
    "icons": {
        "16": "logo.png",
        "32": "logo.png",
        "48": "logo.png",
        "128": "logo.png"
    },
    "background": {
        "service_worker" : "background.js"
    },
    "permissions": [
        "notifications",
        "alarms",
        "storage",
        "unlimitedStorage"
    ]
}
